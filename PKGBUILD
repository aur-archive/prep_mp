# Maintainer: PReP <prep@archlinux.info>

pkgname=prep_mp
pkgver=4.0
pkgrel=1
pkgdesc="A random music at random speed script using mplayer"

arch=('any')
url="http://aur.archlinux.org/"
license=('custom')
depends=('mplayer')
source=($pkgname)

md5sums=('1b85ca2f077af73921b541434053b8b2')

build() {

  echo
  echo "Installing: /usr/bin/prep_mp"
  install -Dm0755 prep_mp $pkgdir/usr/bin/$pkgname
  echo

}

